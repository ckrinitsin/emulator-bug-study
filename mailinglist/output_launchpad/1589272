build qemu from git (6b3532b20b787cbd697a68b383232f5c3b39bd1e)

with this options:

./configure \
    --python=/usr/bin/python2 \
    --prefix=/usr \
    --sysconfdir=/etc \
    --localstatedir=/var \
    --libexecdir=/usr/lib/qemu \
    --target-list=i386-softmmu,x86_64-softmmu,i386-linux-user,x86_64-linux-user \
    --audio-drv-list='pa alsa' \
    --enable-linux-aio \
    --enable-seccomp \
    --enable-tpm \
    --enable-modules \
    --disable-sdl \
    --disable-gtk \
    --disable-spice \
    --disable-rbd \
    --disable-libiscsi \
    --disable-libnfs \
    --disable-smartcard \
    --disable-glusterfs \
    --disable-docs \
    --disable-vnc{,-sasl,-jpeg,-png} \
    --disable-guest-agent

